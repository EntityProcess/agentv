{
  "name": "@agentevo/core",
  "version": "0.0.1",
  "description": "Primitive runtime components for AgentEvo",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "src/index.d.ts"
  ],
  "dependencies": {
    "@ax-llm/ax": "^14.0.39",
    "ai": "^5.0.50",
    "subagent": "^0.3.2",
    "yaml": "^2.6.0",
    "zod": "^3.23.8"
  }
}
